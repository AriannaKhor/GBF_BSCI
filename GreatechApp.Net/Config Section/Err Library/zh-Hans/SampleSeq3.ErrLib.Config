<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="Err_Lib" type="ConfigManager.ErrorConfig, ConfigManager, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"/>
  </configSections>
  <Err_Lib>
    <ErrTable>
      <clear />
	  <Alarm Code="0" Station="SampleSeq3" AlarmType="故障" Cause="电机警报： 电机警报信号已启动"
        Recovery="请检查电机驱动器上的警报代码。 如果此问题持续存在，请联系制造商。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="">
      </Alarm>
      <Alarm Code="1" Station="SampleSeq3" AlarmType="故障" Cause="电机未准备: 准备信号未开"
        Recovery="1) 检查接线。&#xD;&#xA;2) 确保错误计时器参数设置长于电机完成归位操作所需的时间。&#xD;&#xA;3) 恢复后系统将再次执行归位。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="">
      </Alarm>
      <Alarm Code="2" Station="SampleSeq3" AlarmType="故障" Cause="电机没就位: 动作循环已超时，在错误计时器结束后，就位信号保持关闭。"
        Recovery="1) 检查接线。&#xD;&#xA;2) 确定发出运作命令时是否有实际物理在运作。&#xD;&#xA;3) 确保错误计时器参数设置长于电机完成归位操作所需的时间。&#xD;&#xA;4) 恢复后系统将再次执行归位。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="">
      </Alarm>
      <Alarm Code="3" Station="SampleSeq3" AlarmType="故障" Cause="击中前向限位: 动作循环超时，触发前向限位信号。"
        Recovery="1) 确保前向限位传感器运作正常。&#xD;&#xA;2) 您可能需要手动将负载向后移，远离限位传感器。&#xD;&#xA;3) 恢复后系统将再次执行归位。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="Sample.jpg">
      </Alarm>
      <Alarm Code="4" Station="SampleSeq3" AlarmType="故障" Cause="击中反向限位: 动作循环超时，触发反向限位信号。"
        Recovery="1) 确保反向限位传感器运作正常。&#xD;&#xA;2) 您可能需要手动将负载向后移，远离限位传感器。&#xD;&#xA;3) 恢复后系统将再次执行归位。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="">
      </Alarm>
      <Alarm Code="5" Station="SampleSeq3" AlarmType="故障" Cause="未定义的电机错误。"
        Recovery="1) 请检查电机驱动器上的警报代码。 如果此问题持续存在，请联系制造商。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="">
      </Alarm>
      <Alarm Code="6" Station="SampleSeq3" AlarmType="故障" Cause="电机警报未解除： 电机警报信号未解除"
        Recovery="1) 检查接线。&#xD;&#xA;2) 确保重置警报信号启开的时间够长以重置电机警报。&#xD;&#xA;3) 恢复后系统将再次执行归位。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="">
      </Alarm>
	  <Alarm Code="7" Station="SampleSeq" AlarmType="故障" Cause="测试错误 1"
        Recovery="1) 检查接线。&#xD;&#xA;2) 确保重置警报信号启开的时间够长以重置电机警报。&#xD;&#xA;3) 恢复后系统将再次执行归位。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="">
      </Alarm>
	  <Alarm Code="8" Station="SampleSeq" AlarmType="故障" Cause="代码阅读器连接失败"
        Recovery="1) 请检查代码阅读器连接。"
        RetestOption="false" RetestDefault="true" IsStoppage="true" PicPath="">
      </Alarm>
    </ErrTable>
  </Err_Lib>
</configuration>
